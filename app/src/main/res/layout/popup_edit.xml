<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/popup_edit"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/popup_curved">


    <Button
        android:id="@+id/cancel"
        android:layout_width="108dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView7"
        android:layout_marginStart="46dp"
        android:layout_marginLeft="46dp"
        android:layout_marginTop="45dp"
        android:layout_toEndOf="@+id/ok"
        android:layout_toRightOf="@+id/ok"
        android:text="cancel" />

    <EditText
        android:id="@+id/LastChange"
        android:layout_width="277dp"
        android:layout_height="45dp"
        android:layout_below="@+id/firstChange"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="43dp"
        android:layout_marginEnd="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginBottom="100dp"
        android:layout_toEndOf="@+id/textView"
        android:layout_toRightOf="@+id/textView"
        android:background="@drawable/popup_editbox_border_line"
        android:gravity="center" />

    <EditText
        android:id="@+id/firstChange"
        android:layout_width="278dp"
        android:layout_height="45dp"
        android:layout_below="@+id/telephone"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="43dp"
        android:layout_marginEnd="5dp"
        android:layout_marginRight="5dp"
        android:layout_toEndOf="@+id/textView"
        android:layout_toRightOf="@+id/textView"
        android:background="@drawable/popup_editbox_border_line"
        android:gravity="center" />

    <EditText
        android:id="@+id/telephone"
        android:layout_width="279dp"
        android:layout_height="45dp"
        android:layout_alignParentTop="true"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="38dp"
        android:layout_marginEnd="5dp"
        android:layout_marginRight="5dp"
        android:layout_toEndOf="@+id/textView"
        android:layout_toRightOf="@+id/textView"
        android:background="@drawable/popup_editbox_border_line"
        android:gravity="center" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="99dp"
        android:layout_height="45dp"
        android:layout_below="@+id/textView3"
        android:layout_marginStart="5dp"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="43dp"
        android:gravity="center|left"
        android:text="Last Name"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="99dp"
        android:layout_height="45dp"
        android:layout_below="@+id/textView"
        android:layout_marginStart="5dp"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="43dp"
        android:gravity="center|left"
        android:text="First Name"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="99dp"
        android:layout_height="45dp"
        android:layout_alignParentTop="true"
        android:layout_marginStart="5dp"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="38dp"
        android:gravity="center|left"
        android:text="Telephone Number"
        android:textSize="18sp" />

    <Button
        android:id="@+id/ok"
        android:layout_width="108dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView7"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="33dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="45dp"
        android:text="Ok" />

</RelativeLayout>